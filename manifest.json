{
    "name": "No StackExchange Editor Keyboard Shortcuts",
    "manifest_version": 3,
    "version": "1.4",
    "icons": { "16": "icon/16.png",
               "48": "icon/48.png",
               "128": "icon/128.png"},
    "content_scripts": [{
        "js": ["index.js"],
        "matches": [
            "*://*.stackoverflow.com/*",
            "*://*.stackexchange.com/*",
            "*://*.serverfault.com/*",
            "*://*.superuser.com/*",
            "*://*.askubuntu.com/*",
            "*://*.mathoverflow.net/*",
            "*://stackapps.com/*"
        ]
    }]
}
