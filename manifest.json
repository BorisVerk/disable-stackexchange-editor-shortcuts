{
    "name": "No Stack Exchange Editor Keyboard Shortcuts",
    "manifest_version": 3,
    "version": "1.6",
    "icons": { "16": "icon/16.png",
               "48": "icon/48.png",
               "128": "icon/128.png"},
    "content_scripts": [{
        "js": ["index.js"],
        "matches": [
            "*://*.stackoverflow.com/*",
            "*://*.stackexchange.com/*",
            "*://*.serverfault.com/*",
            "*://*.superuser.com/*",
            "*://*.askubuntu.com/*",
            "*://*.mathoverflow.net/*",
            "*://stackapps.com/*"
        ]
    }],
    "browser_specific_settings": {
        "gecko": {
            "id": "foooooooooooooooooooooo@example.com"
        }
    }
}
